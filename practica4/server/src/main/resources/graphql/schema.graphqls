type EoloPlant {
    id: ID
    city: String!
    planning: String
}

input EoloPlantInput {
    id: ID
    city: String!
    planning: String
}

type Query {
    eoloPlants: [EoloPlant]
    eoloPlant(id: ID): EoloPlant
}

type Mutation {
    createEoloPlant(eoloPlant: EoloPlantInput): EoloPlant
    updateEoloPlant(eoloPlant: EoloPlantInput!): EoloPlant
}

type Subscription {
    eoloPlants: [EoloPlant]
    eoloPlant(id: ID): EoloPlant
}
